@model OrderProduct

@{
    Layout = null;
    ViewData["Title"] = "Browse";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Perníček - @ViewData["Title"]</title>

    <environment names="Development">
        <!--     Fonts and icons     -->
        <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
        <!-- CSS Files -->
        <link href="~/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/css/sweetalert2.min.css" rel="stylesheet" />

        <!-- Material Design stylesheet -->
        <link href="~/css/material-kit.min.css?v=1.1.1" rel="stylesheet" />


        <!-- Custom Nav stylesheet -->
        <link href="~/css/vertical-nav.css" rel="stylesheet" />
        <link href="~/css/custom-vertical-nav.css" rel="stylesheet" />

        <!-- Custom stylesheet -->
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment names="Staging,Production">
    </environment>
</head>
<body>
    <!-- Navbar -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" onclick="cancelOrder();">Perníček</a>
            </div>
            <div class=" collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right text-center">
                    <li>
                        <a class="btn btn-danger btn-round" onclick="cancelOrder();">
                            <i class="material-icons">cancel</i> Cancel Order
                        </a>
                    </li>
                    <li>
                        <a asp-controller="Order" asp-action="Order" class="btn btn-warning btn-round">
                            <i class="material-icons">mode_edit</i> Edit Order
                        </a>
                    </li>
                    <li>
                        <a class="btn btn-success btn-round" onclick="confirmOrder();">
                            <i class="material-icons">done</i> Confirm Order
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- End Navbar -->

    <form id="Summary-Form" asp-controller="Order" asp-action="FinishOrder" method="post">
    <div class="index-page">
        <!-- Header -->
        <div class="page-header header-custom header-filter" data-parallax="true" style="background-color: red;">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <h1 class="title">Summary</h1>
                        <h4>Complete your purchase.</h4>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Header -->

        <!-- Content -->
        <div class="main main-raised" style="margin-bottom: 5%; padding-bottom: 5%;">
            <div class="container-fluid">
                <!-- Shipping and payment -->
                <div class="section">
                    <div class="row">

                        <!-- Shipping Address -->
                        <div class="col-md-4 col-md-offset-1">
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h2 class="title">Shipping Address</h2>
                                </div>
                            </div>
                                        <div class="form-group label-floating is-empty">
                                            <span>Street</span><span class="data">@Html.DisplayFor(modelItem => modelItem.street)</span>
                                        </div>
                                        <hr />
                                        <div class="form-group label-floating is-empty">
                                            <span>House Number</span><span class="data">@Html.DisplayFor(modelItem => modelItem.house_number)</span>
                                        </div>
                                        <hr />
                                        <div class="form-group label-floating is-empty">
                                            <span>City</span><span class="data">@Html.DisplayFor(modelItem => modelItem.city)</span>
                                        </div>
                                        <hr />
                                        <div class="form-group label-floating is-empty">
                                            <span>Country</span><span class="data">@Html.DisplayFor(model => model.nameCountry)</span>
                                        </div>
                                        <hr />
                                        <div class="form-group label-floating is-empty">
                                            <span>Postal Code</span><span class="data">@Html.DisplayFor(modelItem => modelItem.post_code)</span>
                                        </div>
                        </div>
                        <!-- End Shipping Address -->

                        <div class="col-md-4 col-md-offset-2">
                            <div class="row">
                                <div class="col-md-12 text-center">
                                    <h2 class="title">Shipping & Payment Option</h2>
                                </div>
                            </div>
                            <div class="row">

                                <!-- Shipping Option -->
                                <div id="land" class="col-md-6 shipping-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Shipping Option</h6>
                                        <div class="icon">
                                            <i class="material-icons">local_shipping</i>
                                        </div>
                                        <h6 class="category">By land</h6>
                                        <h3 class="card-title">FREE</h3>
                                    </div>
                                </div>
                                <div id="sea" class="col-md-6 shipping-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Shipping Option</h6>
                                        <div class="icon">
                                            <i class="material-icons">directions_boat</i>
                                        </div>
                                        <h6 class="category">By sea</h6>
                                        <h3 class="card-title">&euro; 29</h3>
                                    </div>
                                </div>
                                <div id="air" class="col-md-6 shipping-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Shipping Option</h6>
                                        <div class="icon">
                                            <i class="material-icons">local_airport</i>
                                        </div>
                                        <h6 class="category">By air</h6>
                                        <h3 class="card-title">&euro; 69</h3>
                                    </div>
                                </div>
                                <div id="ultra" class="col-md-6 shipping-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Shipping Option</h6>
                                        <div class="icon">
                                            <i class="fa fa-rocket"></i>
                                        </div>
                                        <h6 class="category">Ultradimensional transit</h6>
                                        <h3 class="card-title">&euro; 159</h3>
                                    </div>
                                </div>
                                <input id="shipping-value" type="hidden" value="@Model.ShippingOptionNumber"/>
                                <!-- End Shipping Option -->

                                <!-- Payment Option -->
                                <div id="cash-option" class="col-md-6 summary-option payment-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Payment Option</h6>
                                        <div class="icon">
                                            <i class="fa fa-money" aria-hidden="true"></i>
                                        </div>
                                        <h3 class="card-title">Cash</h3>
                                    </div>
                                </div>
                                <div id="internet-option" class="col-md-6 summary-option payment-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Payment Option</h6>
                                        <div class="icon">
                                            <i class="material-icons">credit_card</i>
                                        </div>
                                        <h3 class="card-title">Internet Cash</h3>
                                    </div>
                                </div>
                                <div id="crypto-option" class="col-md-6 summary-option payment-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Payment Option</h6>
                                        <div class="icon">
                                            <i class="fa fa-btc" aria-hidden="true"></i>
                                        </div>
                                        <h3 class="card-title">Crypto Cash</h3>
                                    </div>
                                </div>
                                <div id="soul-option" class="col-md-6 summary-option payment-option">
                                    <div class="card card-pricing card-plain">
                                        <h6 class="category">Payment Option</h6>
                                        <div class="icon">
                                            <i class="material-icons">whatshot</i>
                                        </div>
                                        <h3 class="card-title">Human Soul</h3>
                                    </div>
                                </div>
                                <input id="payment-value" type="hidden" value="@Model.PaymentMethodNumber"/>
                                <!-- End Payment Option -->

                            </div>
                        </div>
                    </div>

                </div>
                <!-- End Shipping and payment -->

                <!-- Shopping Bag -->
                <div class="section">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2 text-center">
                            <h2 class="title">Shopping Bag</h2>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-10 col-md-offset-1">
                            <div class="table-responsive">
                                        <table class="table table-shopping">
                                                <thead>
                                                    <tr>
                                                        <th class="text-center"></th>
                                                        <th>Product</th>
                                                        <th class="th-description">Color</th>
                                                        <th class="th-description">Size</th>
                                                        <th class="text-center">Price</th>
                                                        <th class="text-center">Quantity</th>
                                                        <th class="text-center">Amount</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                @foreach (var item in Model.OrdProd) {
                                                    <tr>
                                                        <td>
                                                            <div class="img-container">
                                                                <img src="@Html.DisplayFor(modelItem => item.image)" alt="...">
                                                            </div>
                                                        </td>
                                                        <td class="td-name">
                                                            <a href="#item_url">@Html.DisplayFor(modelItem => item.nameProduct)</a>
                                                            <br />
                                                            <small>@Html.DisplayFor(modelItem => item.Firm)</small>
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.colour)
                                                        </td>
                                                        <td>
                                                            @Html.DisplayFor(modelItem => item.size)
                                                        </td>
                                                        <td class="table-number text-center">
                                                            <small>&euro; </small>@Html.DisplayFor(modelItem => item.Price)
                                                        </td>
                                                        <td class="table-number text-center">
                                                            @Html.DisplayFor(modelItem => item.quantity)
                                                        </td>
                                                        <td class="table-number text-center">
                                                            <small>&euro;</small> @Html.DisplayFor(modelItem => item.amount)
                                                        </td>
                                                    </tr>
                                                }
                                                <tr>
                                                    <td></td>
                                                    <td class="td-total">
                                                        Total Item Price
                                                    </td>
                                                    <td class="td-price">
                                                        <small>&euro; </small>@Model.OverallPrice
                                                    </td>
                                                    <td class="td-total">
                                                        Shipment Price
                                                    </td>
                                                    <td class="td-price">
                                                        <small>&euro; </small> @Model.ShippingPrice
                                                    </td>
                                                    <td class="td-total table-number">
                                                        Total Overall
                                                    </td>
                                                    <td class="td-price">
                                                        <small>&euro; </small>@Model.OverallPriceWithShipping
                                                    </td>
                                                </tr>
                                                </tbody>
                                            </table>
                                        </div>
                        </div>
                    </div>
                </div>
                <!-- End Shopping Bag -->

                <!-- Confirm, Edit or Cancel -->
                <div class="section">
                    <div class="row">
                        <div class="col-md-12 text-center">
                            <a data-toggle="modal" data-target="#terms-agreement">By confirming order you agree to terms and agreement.</a>
                            <br/>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12 text-center">
                                <a class="btn btn-lg btn-danger" onclick="cancelOrder();">
                                    <i class="material-icons">cancel</i> Cancel Order
                                </a>
                                <a asp-controller="Order" asp-action="Order" class="btn btn-warning btn-lg">
                                    <i class="material-icons">mode_edit</i> Edit Order
                                </a>
                                <a  class="btn btn-lg btn-success" onclick="confirmOrder();">
                                    <i class="material-icons">done</i> Confirm Order
                                </a>
                        </div>
                    </div>
                </div>
                <!-- End Confirm, Edit or Cancel -->

            </div>
        </div>
    <!-- End Content -->

    </div>
        <input type="hidden" asp-for="@Model.id_ad" />
        <input type="hidden" asp-for="@Model.street" />
        <input type="hidden" asp-for="@Model.house_number" />
        <input type="hidden" asp-for="@Model.city" />
        <input type="hidden" asp-for="@Model.post_code" />
        <input type="hidden" asp-for="@Model.ShippingOption" />
        <input type="hidden" asp-for="@Model.Payment" />
        <input type="hidden" asp-for="@Model.codeCountry" />
        <input type="hidden" asp-for="@Model.OverallPrice" />
    </form>

