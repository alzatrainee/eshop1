@model IEnumerable<Catalog.Dal.Entities.Product>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>


<table class="table">
    <tbody>
      @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.price)
                </td>
            </tr>
        }
        
    </tbody>
</table>